{
    "name": "lenny4/doctrine-merge-persistent-collection",
    "description": "Merge persistence collection when update entity",
    "keywords": [
        "Lenny4",
        "doctrine-merge-persistent-collection"
    ],
    "homepage": "https://github.com/lenny4/doctrine-merge-persistent-collection",
    "license": "MIT",
    "authors": [
        {
            "name": "Lenny4",
            "email": "alexandre.beaujour@hotmail.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.13",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/framework-bundle": "^6.1",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lenny4\\DoctrineMergePersistentCollectionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lenny4\\DoctrineMergePersistentCollectionBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
